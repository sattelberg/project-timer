<div class="small-1 small-centered columns">
  <h1>Users</h1>
</div>
<table class="small-12 small-centered colums">
  <tr>
   <th>id</th>
   <th>UserName</th>
   <th>Email</th>
   <th>Created At</th>
   <th>Updated At</th>
  </tr>
  <% @users.each do |user| %>
    <tr>
      <td><%=user.id%></td>
      <td><%=link_to user.username, {:controller => :users, :action => 'show', :id => user.id},:class=>"btn-link"%></td>
      <td><%=user.email%></td>
      <td><%=user.created_at%></td>
      <td><%=user.updated_at%></td>
      <td><%=link_to 'edit', {:controller => :users, :action => 'edit', :id => user.id},:class=>"button"%></td>
      <td><%=button_to 'Delete', {:controller => :users, :action => 'destroy', :id => user.id}, {:method => :delete, :class => "button"}%></td>
    </tr>
  <% end %>
</table>
